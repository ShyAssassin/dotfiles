[user]
    name = [Assassin]
    username = ShyAssassin
    email = ShyAssassin@assassin.dev
[core]
    eol = lf
    autocrlf = false
    editor = code --wait
    attributesfile = ~/.gitattributes_global
[color]
    ui = auto
    status = auto
[fetch]
    prune = true
[filter "lfs"]
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
